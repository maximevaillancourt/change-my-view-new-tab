{
	"name": "/r/changemyview on New Tab page",
	"description" : "Get one of the top CMV entry (from the past 24 hours) on your New Tab page.",
	"version": "1.2.0",
	"applications": {
		"gecko": {
			"strict_min_version": "54.0"
		}
	},
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon.png",
